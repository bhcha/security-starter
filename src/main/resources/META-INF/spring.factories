# Auto Configuration
org.springframework.boot.autoconfigure.EnableAutoConfiguration=\
  com.ldx.hexacore.security.config.HexacoreSecurityAutoConfiguration,\
  com.ldx.hexacore.security.config.autoconfigure.PersistenceAutoConfiguration,\
  com.ldx.hexacore.security.config.autoconfigure.ApplicationLayerAutoConfiguration,\
  com.ldx.hexacore.security.config.autoconfigure.TokenProviderAutoConfiguration,\
  com.ldx.hexacore.security.config.autoconfigure.SecurityFilterAutoConfiguration,\
  com.ldx.hexacore.security.config.autoconfigure.HexacoreSecurityFilterAutoConfiguration

# Environment Post Processors
org.springframework.boot.env.EnvironmentPostProcessor=\
  com.ldx.hexacore.security.config.properties.SecurityConfigurationPropertiesPostProcessor

# Failure Analyzers
org.springframework.boot.diagnostics.FailureAnalyzer=\
  com.ldx.hexacore.security.config.support.SecurityAuthFailureAnalyzer